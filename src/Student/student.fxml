<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Student.StudentController">
    <children>
        <Label layoutX="50.0" layoutY="57.0" text="Name:" />
        <TextField fx:id="stuName" layoutX="117.0" layoutY="52.0" prefHeight="26.0" prefWidth="229.0" />
        <Label layoutX="29.0" layoutY="93.0" text="Surname:" />
        <TextField fx:id="stuSName" layoutX="117.0" layoutY="88.0" prefHeight="26.0" prefWidth="229.0" />
        <Label layoutX="52.0" layoutY="130.0" text="Email:" />
        <TextField fx:id="stuMail" layoutX="117.0" layoutY="125.0" prefHeight="26.0" prefWidth="229.0" />
      <Label layoutX="72.0" layoutY="169.0" text="ID:" />
      <TextField fx:id="stuID" layoutX="117.0" layoutY="164.0" prefHeight="26.0" prefWidth="229.0" />
        <Button layoutX="235.0" layoutY="267.0" mnemonicParsing="false" onAction="#doneButton" prefHeight="26.0" prefWidth="111.0" text="Done" />
        <Button layoutX="235.0" layoutY="304.0" mnemonicParsing="false" onAction="#cancelButton" prefHeight="26.0" prefWidth="111.0" text="Cancel" />
        <Label fx:id="errorLabel" layoutX="44.0" layoutY="272.0" textFill="RED">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
